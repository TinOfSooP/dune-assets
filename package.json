    "@tamtamchik/json-deep-sort": "^1.1.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "cli-progress": "^3.12.0",
    "jimp": "^0.22.12",
    "markdown-to-jsx": "^7.4.7",
    "p-limit": "^5.0.0",
    "playwright": "^1.44.1",
    "react": "^18.3.1",
    "react-children-utilities": "^2.10.0",
    "react-dom": "^18.3.1",
    "recursive-readdir-files": "^2.3.1",
